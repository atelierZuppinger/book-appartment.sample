  <body class="lang-{{ app.request.getLocale() }} {% block body_class %}{% endblock %}">

    <header>

      <aside id="photoGallery" class="ambiancePicture">
        {% block ambiance_picture %}
          <ul id="galleryImages">
            <li class="current">
              <figure>
                <img src="{{ asset('imgs/heidi_fond_home_01.jpg') }}" alt="HEIDI-Alpen" />
              </figure>
            </li>
            <li>
              <figure>
                <img src="{{ asset('imgs/heidi_fond_home_04.jpg') }}" alt="HEIDI-Alpen" />
              </figure>
            </li>
            <li>
              <figure>
                <img src="{{ asset('imgs/heidi_fond_expert_02.jpg') }}" alt="{{ 'theExpertise.title'|trans }}" />
              </figure>
            </li>
            <li>
              <figure>
                <img src="{{ asset('imgs/heidi_fond_cheese_01.jpg') }}" alt="{{ 'theCheeses.title'|trans }}" />
              </figure>
            </li>
            <li>
              <figure>
                <img src="{{ asset('imgs/heidi_fond_story_03.jpg') }}" alt="{{ 'theStory.title'|trans }}" />
              </figure>
            </li>
          </ul>
          <menu>
            <button class="current" value="1">1</button>
            <button value="2">2</button>
            <button value="3">3</button>
            <button value="4">4</button>
            <button value="5">5</button>
          </menu>

          {#<div id="legends">
            <div class="center-content">
              <span>
                Switzerland’s finest cheeses.<br />
                Discover the legend, soon!
              </span>
              <span>
                Switzerland’s finest cheeses.<br />
