              <span>
                Switzerland’s finest cheeses.<br />
                Discover the legend, soon!
              </span>
              <span>
                Switzerland’s finest cheeses.<br />
                Discover the legend, soon!
              </span>
              <span>
                Switzerland’s finest cheeses.<br />
                Discover the legend, soon!
              </span>
            </div>
          </div>#}
        {% endblock %}
      </aside>
      <div id="topAbsolute">
        <div class="center-content">
          
          <ul id="topMenu" class="noMobile">
            <li>
              <span>{{ 'global.topMenu.langue'|trans }}</span>
              <ul class="subMenu">
              {% if selectedCheese is defined %}
                <li><a href="{{ path( app.request.attributes.get('_route'), {_locale:'en', cheese: selectedCheese}) }}">English</a></li>
                {#<li><a href="{{ path( app.request.attributes.get('_route'), {_locale:'de', cheese: selectedCheese}) }}">Deutsch</a></li>#}
                <li><a href="{{ path( app.request.attributes.get('_route'), {_locale:'ru', cheese: selectedCheese}) }}">Русский</a></li>
                <li><a href="{{ path( app.request.attributes.get('_route'), {_locale:'jp', cheese: selectedCheese}) }}">日本語</a></li>
              {% elseif receipt is defined %}
                <li><a href="{{ path( app.request.attributes.get('_route'), {_locale:'en', receipt: receipt}) }}">English</a></li>
                {#<li><a href="{{ path( app.request.attributes.get('_route'), {_locale:'de', cheese: selectedCheese}) }}">Deutsch</a></li>#}
                <li><a href="{{ path( app.request.attributes.get('_route'), {_locale:'ru', receipt: receipt}) }}">Русский</a></li>
                <li><a href="{{ path( app.request.attributes.get('_route'), {_locale:'jp', receipt: receipt}) }}">日本語</a></li>
              {% else %}
                <li><a href="{{ path( app.request.attributes.get('_route'), {_locale:'en'}) }}">English</a></li>
                {#<li><a href="{{ path( app.request.attributes.get('_route'), {_locale:'de'}) }}">Deutsch</a></li>#}
                <li><a href="{{ path( app.request.attributes.get('_route'), {_locale:'ru'}) }}">Русский</a></li>
                <li><a href="{{ path( app.request.attributes.get('_route'), {_locale:'jp'}) }}">日本語</a></li>
              {% endif %}
              </ul>
            </li>
            <li>
              <a href="{{ path('heidi_alpen_contact') }}">{{ 'global.topMenu.contact'|trans }}</a>
            <li>
                <a href="{{ path('heidi_alpen_files') }}">{{ 'global.topMenu.login'|trans }}</a>      
            </li>
          </ul>


          <a href="{{ path('heidi_alpen_homepage') }}" id="logo">
            <img src="{{ asset('imgs/logo.png') }}" alt="HEIDI-Alpen logo" />
          </a>
          
        </div>

        <nav id="mainNav" class="noMobile">
          {{ knp_menu_render('mainMenu', {'compressed': true, 'depth': 1}) }}
          <div class="clear"></div>
        </nav>

        <div id="mobileNav" class="noDesktop">
          <div id="menuToggler">
            <div class="top"></div>
            <div class="middle"></div>
            <div class="bottom"></div>
          </div>
          <nav>
            <a href="{{ path('heidi_alpen_homepage') }}" id="logo-menu">
              <img src="{{ asset('imgs/logo.png') }}" alt="HEIDI-Alpen logo" />
            </a>
            {{ knp_menu_render('mainMenu', {'compressed': true, 'allow_safe_labels': true}) | raw }}
            <div id="optionNav">
              <select name="language" id="langSwitcher">
                  <option>{{ 'global.topMenu.langue'|trans }}</option>
              {% if selectedCheese is defined %}
                  <option data-href="{{ path( app.request.attributes.get('_route'), {_locale:'en', cheese: selectedCheese}) }}">English</option>
                  {#<option data-href="{{ path( app.request.attributes.get('_route'), {_locale:'de', cheese: selectedCheese}) }}">Deutsch</option>#}
                  <option data-href="{{ path( app.request.attributes.get('_route'), {_locale:'ru', cheese: selectedCheese}) }}">Русский</option>
                  <option data-href="{{ path( app.request.attributes.get('_route'), {_locale:'jp', cheese: selectedCheese}) }}">日本語</option>
                {% elseif receipt is defined %}
                  <option data-href="{{ path( app.request.attributes.get('_route'), {_locale:'en', receipt: receipt}) }}">English</option>
                  {#<option data-href="{{ path( app.request.attributes.get('_route'), {_locale:'de', cheese: selectedCheese}) }}">Deutsch</option>#}
                  <option data-href="{{ path( app.request.attributes.get('_route'), {_locale:'ru', receipt: receipt}) }}">Русский</option>
                  <option data-href="{{ path( app.request.attributes.get('_route'), {_locale:'jp', receipt: receipt}) }}">日本語</option>
                {% else %}
                  <option data-href="{{ path( app.request.attributes.get('_route'), {_locale:'en'}) }}">English</option>
                  {#<option data-href="{{ path( app.request.attributes.get('_route'), {_locale:'de'}) }}">Deutsch</option>#}
                  <option data-href="{{ path( app.request.attributes.get('_route'), {_locale:'ru'}) }}">Русский</option>
                  <option data-href="{{ path( app.request.attributes.get('_route'), {_locale:'jp'}) }}">日本語</option>
                {% endif %}
              </select>
              <div class="clear"></div>
            </div>
          </nav>
        </div>

        {% block after_header %}{% endblock %}
        
      </div>


    </header>

    <main id="content">

      {% block main %}{% endblock %}

    </main>

    <footer>

      <div id="signature">
        <div class="center-content">
          {% block indexH1and2 %}
            <img class="montains fullImg" src="{{ asset('imgs/pied_mont_logo.png') }}" alt="Montains" />
            <strong id="slogan">{{ 'global.footer.slogan'|trans }}</strong>
          {% endblock %}
        </div>
      </div>

      <div id="endOfPagae">
        <div class="center-content">
          <nav id="footerNav">
            {{ knp_menu_render('mainMenu', {'compressed': true, 'allow_safe_labels': true}) | raw }}
            <span class="clear"></span>
          </nav>

        </div>

        <section>
          <div class="center-content">
            <address><strong>© {{ 'global.address.nom'|trans }}</strong> | {{ 'global.address.group'|trans }} · {{ 'global.address.rue'|trans }} · {{ 'global.address.cp'|trans }} {{ 'global.address.ville'|trans }} · {{ 'global.address.pays'|trans }} · Tel. {{ 'global.address.tel'|trans }} · <a href="{{ path('heidi_alpen_contact') }}">{{ 'global.topMenu.contact'|trans }}</a></address>
            {#<a href="#">Login</a>#}
            <span class="clear"></span>
          </div>
        </section>

      </div>
    </footer>

    <div id="mask"></div>

